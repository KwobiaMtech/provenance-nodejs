{
  "name": "provenance-nodejs",
  "version": "1.0.0",
  "main": "lib/src/index.js",
  "module": "lib/src/index.m.js",
  "unpkg": "lib/src/index.umd.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "serve": "nodemon lib/src/index.js",
    "watch": "tsc -w",
    "test": "jest --setupFiles dotenv/config",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn build && jest --config ./test/jest-e2e.json",
    "test:e2e:quick": "FIXTURES_LOAD=false jest --config ./test/jest-e2e.json",
    "lint": "eslint --fix",
    "lint:fix": "eslint src --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/node": "^18.11.11",
    "add": "^2.0.6",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.16",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@cosmjs/stargate": "^0.31.1",
    "@provenanceio/provenance.js": "^0.1.1",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "axios": "^1.2.1",
    "bip39": "^3.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-sorted": "^1.0.14",
    "module-name": "^0.0.1-security",
    "referral-codes": "^2.0.3",
    "ts-jest": "^29.0.3",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
